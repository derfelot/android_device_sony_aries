# mountpoint	fstype	device						[options]

/boot		emmc	/dev/block/bootdevice/by-name/boot
/system		ext4	/dev/block/bootdevice/by-name/system		flags=display="System";wipeingui
/cache		ext4	/dev/block/bootdevice/by-name/cache
/data		ext4	/dev/block/bootdevice/by-name/userdata		flags=length=-16384;encryptable=footer
/external_sd	auto	/dev/block/mmcblk1p1	/dev/block/mmcblk1	flags=display="External SD"
/usb-otg	vfat	/dev/block/sda1		/dev/block/sda		flags=removable;storage;display=USB-OTG
/recovery	emmc	/dev/block/bootdevice/by-name/FOTAKernel	flags=display="Recovery";backup=1
/TA		emmc	/dev/block/bootdevice/by-name/TA		flags=display="!Trim Area!";backup=1
/misc		emmc	/dev/block/bootdevice/by-name/apps_log		flags=display="Misc"
